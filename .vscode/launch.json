{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "vuejs: chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/workbench_web/frontend/src",
            "breakOnLoad": true,
            "sourceMapPathOverrides": {
                "webpack:///src/*": "${webRoot}/*"
            }
        },
        {
            "name": "web poetry py module",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/workbench_web/backend/app/app",
            "module": "workbench_web"
        },
        {
            "name": "web py module",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}/workbench_web/backend/app/app",
            "module": "workbench_web"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "home_workbench module",
            "type": "python",
            "request": "launch",
            "module": "home_workbench",
            "cwd": "${workspaceFolder}\\home_workbench",
            "args": [
                "5"
            ]
        },
        {
            "name": "spd3303c module",
            "type": "python",
            "request": "launch",
            "cwd": "${workspaceFolder}\\home_workbench",
            "module": "spd3303c"
        },
        {
            "name": "Docker: Python - General",
            "type": "docker",
            "request": "launch",
            "preLaunchTask": "docker-run: debug",
            "python": {
                "pathMappings": [
                    {
                        "localRoot": "${workspaceFolder}",
                        "remoteRoot": "/app"
                    }
                ],
                "projectType": "general"
            }
        }
    ]
}
